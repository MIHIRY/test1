program 8


# prime number 
n = int(input("enter a number"))
sum = 0
for i in range(1 , n ) :  # using range
    if(n % i == 0):
     sum = sum + i
    print(sum )
if(sum == n):
  print("prime number")
else:
 print("not a prime")
        
        
        
